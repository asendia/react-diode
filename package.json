  "author": "Traveloka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/traveloka/react-diode/issues"
  },
  "homepage": "https://github.com/traveloka/react-diode#readme",
  "dependencies": {
    "deep-extend": "^0.4.0",
    "object-assign": "^4.0.1",
    "superagent": "^1.4.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "marlint": "^1.5.3"
  },
  "marlint": {
    "ignores": [
      "lib/**"
